{
  "$id": "http://www.knappi.org/handlebars-ng/test/schema",
  "$ref": "#/definitions/HandlebarsTest",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "ContentStatement": {
      "additionalProperties": false,
      "properties": {
        "loc": {
          "$ref": "#/definitions/SourceLocation"
        },
        "original": {
          "type": "string"
        },
        "type": {
          "const": "ContentStatement",
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "loc",
        "original",
        "type",
        "value"
      ],
      "type": "object"
    },
    "HandlebarsTest": {
      "additionalProperties": false,
      "properties": {
        "$schema": {
          "const": "http://www.knappi.org/handlebars-ng/test/schema",
          "type": "string"
        },
        "ast": {
          "$ref": "#/definitions/Program"
        },
        "description": {
          "type": "string"
        },
        "failsInOriginalHandlebars": {
          "type": "boolean"
        },
        "helpers": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object"
        },
        "input": {
          "type": "object"
        },
        "output": {
          "type": "string"
        },
        "template": {
          "type": "string"
        }
      },
      "required": [
        "$schema",
        "description",
        "template",
        "ast",
        "input",
        "output"
      ],
      "type": "object"
    },
    "MustacheStatement": {
      "additionalProperties": false,
      "properties": {
        "escaped": {
          "type": "boolean"
        },
        "loc": {
          "$ref": "#/definitions/SourceLocation"
        },
        "params": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "path": {
          "$ref": "#/definitions/PathExpression"
        },
        "strip": {
          "$ref": "#/definitions/StripFlags"
        },
        "type": {
          "const": "MustacheStatement",
          "type": "string"
        }
      },
      "required": [
        "escaped",
        "loc",
        "params",
        "path",
        "strip",
        "type"
      ],
      "type": "object"
    },
    "PathExpression": {
      "additionalProperties": false,
      "properties": {
        "data": {
          "type": "boolean"
        },
        "depth": {
          "type": "number"
        },
        "loc": {
          "$ref": "#/definitions/SourceLocation"
        },
        "original": {
          "type": "string"
        },
        "parts": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "type": {
          "const": "PathExpression",
          "type": "string"
        }
      },
      "required": [
        "data",
        "depth",
        "loc",
        "original",
        "parts",
        "type"
      ],
      "type": "object"
    },
    "Position": {
      "additionalProperties": false,
      "properties": {
        "column": {
          "type": "number"
        },
        "line": {
          "type": "number"
        }
      },
      "required": [
        "line",
        "column"
      ],
      "type": "object"
    },
    "Program": {
      "additionalProperties": false,
      "properties": {
        "blockParams": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "body": {
          "items": {
            "$ref": "#/definitions/Statement"
          },
          "type": "array"
        },
        "loc": {
          "$ref": "#/definitions/SourceLocation"
        },
        "strip": {
          "additionalProperties": {
            "not": {}
          },
          "type": "object"
        },
        "type": {
          "const": "Program",
          "type": "string"
        }
      },
      "required": [
        "body",
        "loc",
        "strip",
        "type"
      ],
      "type": "object"
    },
    "SourceLocation": {
      "additionalProperties": false,
      "properties": {
        "end": {
          "$ref": "#/definitions/Position"
        },
        "source": {
          "type": "string"
        },
        "start": {
          "$ref": "#/definitions/Position"
        }
      },
      "required": [
        "start",
        "end"
      ],
      "type": "object"
    },
    "Statement": {
      "anyOf": [
        {
          "$ref": "#/definitions/ContentStatement"
        },
        {
          "$ref": "#/definitions/MustacheStatement"
        }
      ]
    },
    "StripFlags": {
      "additionalProperties": false,
      "properties": {
        "close": {
          "type": "boolean"
        },
        "open": {
          "type": "boolean"
        }
      },
      "required": [
        "open",
        "close"
      ],
      "type": "object"
    }
  }
}
