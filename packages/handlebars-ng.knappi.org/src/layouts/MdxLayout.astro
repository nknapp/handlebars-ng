---
import SidebarContents from "@/components/sidebar/SidebarContents.astro";
import type { MarkdownLayoutProps } from "astro";
import Layout from "./default/DefaultLayout.astro";

type Props = MarkdownLayoutProps<{
  title: string;
}>;

const { frontmatter } = Astro.props;
---

<Layout title={frontmatter.title} section={frontmatter.title}>
  <div slot="sidebar">
    <SidebarContents />
  </div>
  <slot />
</Layout>
