{
  "name": "handlebars-ng.knappi.org",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "VITE_FORCE=true yarn generate && astro build",
    "generate:perf": "vite-node scripts/generate-benchmarks.ts",
    "generate": "concurrently -k npm:generate:*",
    "dev": "concurrently -k npm:dev:*",
    "dev:server": "yarn generate:perf && astro dev",
    "dev:types": "tsc --build tsconfig.json --noEmit --watch",
    "dev:unit": "vitest --ui",
    "test": "concurrently -m 1 npm:test:*",
    "test:types": "tsc --build",
    "test:unit": "vitest --run",
    "preview": "astro preview",
    "astro": "astro",
    "---- lerna tasks ----": "",
    "build:watch": "vite build --watch",
    "test-and-build": "concurrently -m 1 --kill-others-on-fail npm:test:* npm:build",
    "clean": "rm dist -rf"
  },
  "files": [
    "ast.d.ts",
    "tests.d.ts",
    "types",
    "tests.mjs"
  ],
  "dependencies": {
